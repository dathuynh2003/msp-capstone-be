{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DbConnectionString": "Server=your-server;Port=your-port;Database=your-db;User Id=your-user;Password=your-password"
  },

  "Stream": {
    "ApiKey": "your-key",
    "ApiSecret": "your-secret",
    "BaseUrl": "your-url",
    "ActionUrl": "your-url"
  },
  "JwtOptions": {
    "Secret": "your-secret",
    "Issuer": "your-issuer",
    "Audience": "your-audience",
    "ExpirationTimeInMinutes": 100
  },
  "Authentication": {
    "Google": {
      "ClientId": "your-google-client-id",
      "ClientSecret": "your-google-client-secret"
    }
  },
  "EmailSettings": {
    "SmtpServer": "your-mail",
    "SmtpPort": 587,
    "Username": "your-username",
    "Password": "your-password",
    "FromEmail": "your-email",
    "FromName": "your-name",
    "EnableSsl": true
  },
  "Gemini": {
    "ApiKey": "your-api-key-here",
    "ModelName": "your-model-name-here"
  }
}
